<% if @message.present? %>
  alert("<%= @message %>");
  $("#p-status-<%= @order.id %>").addClass("bg-<%= @order.status_was %>");
  $("#p-status-<%= @order.id %>").html("<%= @order.status_was %>");
  $("#btn-cancel-order-<%= @order.id %>").remove();
<% else %>
  alert("<%= t ".delete_false" %>");
<% end %>
